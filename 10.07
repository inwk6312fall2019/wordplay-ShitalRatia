def is_anagram(listik1, listik2):
    if len(listik1) == len(listik2):
        listik1 = list(listik1)
        listik2 = list(listik2)
        listik1.sort()
        listik2.sort()
        return listik1 == listik2
    else:
        return False
    
print(is_anagram('orchestra','carthorse'))
print(is_anagram('sport','porta'))
print(is_anagram(' ',' '))
