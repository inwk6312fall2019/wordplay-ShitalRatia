def nested_sum(listik):
    total = 0
    for i in listik:
        if type(i) is list:
           i = nested_sum(i)  
        total += i
    return total

print(nested_sum([1,2,3,[4,5],[10]]))
print(nested_sum([]))
print(nested_sum([[1],[2],[3],[4],[5],[10]]))
print(nested_sum([1,2,3,[-4,-5],[10]]))
print(nested_sum([1,2,3,[[-4,[-5]],[10]]]))
